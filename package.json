    "bcrypt": "^3.0.0",
    "body-parser": "^1.17.2",
    "bootstrap": "4.0.0-alpha.6",
    "brain.js": "^1.2.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.4",
    "express-jwt": "^5.3.1",
    "if-env": "^1.0.4",
    "jsonfile": "^4.0.0",
    "jsonwebtoken": "^8.3.0",
    "mdbreact": "^4.6.0",
    "moment": "^2.22.2",
    "mongoose": "^5.0.17",
    "natural": "^0.6.1",
    "react": "^16.4.1",
